<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tugas.www.finder">

    <application
        android:name=".di.FinderApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PassCodeActivity"></activity>
        <activity android:name=".SetPassCodeActivity" />

        <receiver
            android:name=".AlarmReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".expenselimit.ExpenseLimitActivity" />
        <activity android:name=".plan.SetIncomeActivity" />
        <activity android:name=".plan.SetExpenseActivity" />
        <activity android:name=".inputmonetary.InputIncomeActivity" />
        <activity android:name=".inputmonetary.InputExpenseActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>